{
  "name": "hippo",
  "tree": {
    "$className": "DataModel",
    "HttpService": {
      "$className": "HttpService",
      "$properties": {
        "HttpEnabled": true
      }
    },

    "ServerScriptService": {
      "$className": "ServerScriptService",

      "main": {
        "$path": "main/main.server.lua"
      }
    },

    "StarterPlayer": {
      "$className": "StarterPlayer",

      "StarterPlayerScripts": {
        "$className": "StarterPlayerScripts",

        "main": {
          "$path": "main/main.client.lua"
        }
      }
    },

    "ReplicatedStorage": {
      "$className": "ReplicatedStorage",

      "Packages": {
        "$path": "Packages",

        "Hippo": {
          "$path": "src"
        },

        "RemoteEvent": {
          "$className": "RemoteEvent"
        }
      }
    }
  }
}